        android:textAllCaps="false"
        android:textColor="@color/colorYellow"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonLeftH"
        app:layout_constraintEnd_toStartOf="@+id/guideline5H"
