<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorWhite"
    android:clickable="true"
    android:contentDescription="@string/minus_sign"
    android:focusable="true"
    android:orientation="vertical">

    <Button
        android:id="@+id/buttonLeftH"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/back_arrow"
        android:text="@string/back"
        android:textColor="@android:color/background_light"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline5H"
        app:layout_constraintStart_toStartOf="@+id/guideline4H"
        tools:layout_conversion_absoluteHeight="72dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/buttonRightH"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/forward_arrow"
        android:text="@string/forward"
        android:textColor="@android:color/background_light"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline5H"
        tools:layout_conversion_absoluteHeight="72dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/buttonA"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/a"
        android:textAllCaps="false"
        android:textColor="@color/colorYellow"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonMinusH"
        app:layout_constraintEnd_toStartOf="@+id/guideline2H"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/buttonB"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/b"
        android:textAllCaps="false"
        android:textColor="@color/colorYellow"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/button_DeleteH"
        app:layout_constraintEnd_toStartOf="@+id/guideline3H"
        app:layout_constraintStart_toStartOf="@+id/guideline2H"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/buttonC"
        android:layout_width="0dp"
        android:layout_height="48dp"
