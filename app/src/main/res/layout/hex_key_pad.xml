    <Button
        android:id="@+id/button3H"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/three"
        android:text="@string/three"
        android:textColor="@color/colorYellow"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/button8H"
        app:layout_constraintEnd_toStartOf="@+id/guideline4H"
        app:layout_constraintStart_toStartOf="@+id/guideline3H"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/button4H"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/four"
        android:text="@string/four"
        android:textColor="@color/colorYellow"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/button9H"
        app:layout_constraintEnd_toStartOf="@+id/guideline5H"
        app:layout_constraintStart_toStartOf="@+id/guideline4H"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/button5H"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/five"
        android:text="@string/five"
        android:textColor="@color/colorYellow"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonZeroH"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline5H"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/button6H"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/six"
        android:text="@string/six"
        android:textColor="@color/colorYellow"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonA"
        app:layout_constraintEnd_toStartOf="@+id/guideline2H"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="0dp" />

    <Button
        android:id="@+id/button7H"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/colorDevilRed"
        android:contentDescription="@string/seven"
        android:text="@string/seven"
        android:textColor="@color/colorYellow"
